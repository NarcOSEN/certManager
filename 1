from cryptography import x509
import os


def main():
    print("hello world")

    file_paths = []
    for root, dir, files in os.walk("./crypto_mess/"):
        for file in files:
            full_path = os.path.join(root, file)
            file_paths.append(full_path)

    for path in file_paths:
        with open(path, "rb"") as file:
            print(file.readall())


if __name__ == "__main__":
    main()
